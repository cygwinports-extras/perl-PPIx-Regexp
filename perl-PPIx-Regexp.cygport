CPAN_AUTHOR=wyant
inherit perl

NAME="perl-PPIx-Regexp"
VERSION=0.040
RELEASE=1
CATEGORY="Perl"
SUMMARY="Perl PPI regex parser module"
DESCRIPTION="This package parses regular expressions as they appear in Perl
scripts, generating a structure similar to the structure generated by PPI when
it parses a Perl script, and navigable in much the same way."

ARCH=noarch
